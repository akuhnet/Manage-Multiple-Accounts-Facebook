'///////////////////facebook comment///////////////////'
TAB T=1
SET !VAR1 EVAL("var randomNumber=Math.floor(Math.random()*3 + 2); randomNumber;")
SET !VAR3 "https://m.facebook.com"
URL GOTO={{!var3}}
WAIT SECONDS={{!var1}}
'change 70 with the number of your comment'
SET !DATASOURCE comment.csv
SET !VAR4 EVAL("var randomNumber=Math.floor(Math.random()*70 + 1); randomNumber;")
TAG POS=1 TYPE=A ATTR=TXT:*<SP>Comments
WAIT SECONDS={{!var1}}
EVENT TYPE=CLICK SELECTOR="#composerInput" BUTTON=0
EVENTS TYPE=KEYPRESS SELECTOR="#composerInput" CHARS="{{!COL1}}"
WAIT SECONDS={{!var1}}
TAG POS=1 TYPE=BUTTON FORM=ID:* ATTR=NAME:submit
WAIT SECONDS=10
URL GOTO={{!var3}}
WAIT SECONDS={{!var1}}
TAG POS=1 TYPE=A ATTR=TXT:*<SP>Comments
WAIT SECONDS={{!var1}}
